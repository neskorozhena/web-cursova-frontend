{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.12.3",
    "@mui/material": "^5.5.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-dom": "^17.0.13",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "a11y-checker": "^2.2.1",
    "axios": "^0.26.0",
    "axios-mock-adapter": "^1.21.4",
    "base64topdf": "^1.1.8",
    "favicon-badge-notify": "^1.1.5",
    "formik": "^2.2.9",
    "html-react-parser": "^1.4.8",
    "husky": "^8.0.1",
    "i": "^0.3.7",
    "i18next": "^21.6.13",
    "is-ip": "^5.0.0",
    "moment": "^2.29.4",
    "npm": "^9.6.6",
    "react": "^17.0.2",
    "react-datepicker": "^4.8.0",
    "react-dom": "^17.0.2",
    "react-h5-audio-player": "^3.8.4",
    "react-hash-scroll": "^1.4.7",
    "react-i18next": "^11.15.6",
    "react-icons": "^4.8.0",
    "react-joyride": "^2.5.2",
    "react-phone-number-input": "^3.1.46",
    "react-player": "^2.11.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "react-use": "^17.4.0",
    "react-verification-code-input": "^1.2.9",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "sass": "^1.49.9",
    "styled-components": "^5.3.3",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run stylelint && npm run prettier && npm run eslint && react-scripts build ",
    "eject": "react-scripts eject",
    "stylelint": "stylelint \"**/*.scss\"",
    "prettier": "prettier --check . --ignore-path ./.prettierignore",
    "eslint": "eslint \"**/*.{ts,tsx}\" --ignore-path ./.eslintignore",
    "stylelint-fix": "stylelint --fix \"**/*.scss\"",
    "prettier-fix": "prettier . --write --ignore-path ./.prettierignore",
    "eslint-fix": "eslint \"**/*.{ts,tsx}\" --fix --ignore-path ./.eslintignore",
    "freshinstall": "npm run clean && npm i",
    "prepare": "husky install",
    "test": "react-scripts test",
    "test-coverage": "npm test -- --coverage --watchAll=false"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.js",
      "!src/**/*.template.{js,jsx}",
      "!src/hocs/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "text-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cfaester/enzyme-adapter-react-18": "^0.7.0",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.24",
    "enzyme": "^3.11.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0"
  },
  "engines": {
    "node": "^16.14.2"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
